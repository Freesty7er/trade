	<totalField>
		<dataPath>НашаДоля</dataPath>
		<expression>ВЫБОР КОГДА Сумма(Количество) = 0 ИЛИ Сумма(КоличествоКонкуренты) = 0 ТОГДА
0
ИНАЧЕ
ВЫБОР КОГДА (Сумма(Количество)+Сумма(КоличествоКонкуренты)) = 0 
ТОГДА 0 
ИНАЧЕ Сумма(Количество)/(Сумма(Количество)+Сумма(КоличествоКонкуренты)) * 100
КОНЕЦ
КОНЕЦ</expression>

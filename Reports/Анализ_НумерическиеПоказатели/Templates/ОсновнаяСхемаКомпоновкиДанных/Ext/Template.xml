			<field xsi:type="DataSetFieldField">
				<dataPath>ГруппаПродукта</dataPath>
				<field>ГруппаПродукта</field>
				<title xsi:type="v8:LocalStringType">
					<v8:item>
						<v8:lang>ru</v8:lang>
						<v8:content>Группа продукта</v8:content>
